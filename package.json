{
  "name": "elite-time",
  "productName": "EliteTime",
  "version": "0.3.0",
  "description": "Unnoficial desktop EliteTorrent.net client",
  "license": "MIT",
  "repository": "https://github.com/telpalbrox/EliteTime",
  "author": {
    "name": "Alberto Luna",
    "email": "alberto.luna.95@gmail.com"
  },
  "engines": {
    "node": ">=4"
  },
  "electronVersion": "0.37.8",
  "scripts": {
    "start": "grunt build-dev && electron .",
    "package-all": "grunt build-dev && electron-packager . EliteTime --out=dist --ignore='^/dist' --prune --asar --all",
    "package-linux": "grunt build-dev && electron-packager . EliteTime --out=dist --ignore='^/dist' --prune --asar --platform=linux --arch=all",
    "package-darwin": "grunt build-dev && electron-packager . EliteTime --out=dist --ignore='^/dist' --prune --asar --platform=darwin --arch=all",
    "package-windows": "grunt build-dev && electron-packager . EliteTime --out=dist --ignore='^/dist' --prune --asar --platform=win32 --arch=all",
    "dev": "grunt dev",
    "dist": "electron-builder"
  },
  "keywords": [
    "electron-app",
    "torrent"
  ],
  "dependencies": {
    "axios": "0.16.2",
    "bootstrap": "3.3.7",
    "cheerio": "1.0.0-rc.1",
    "electron-debug": "1.2.0",
    "flux": "3.1.2",
    "history": "4.6.3",
    "jquery": "3.2.1",
    "keymirror": "0.1.1",
    "lodash": "4.17.4",
    "peerflix": "0.36.3",
    "prop-types": "^15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-router-dom": "4.1.1",
    "request": "2.81.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-preset-react": "6.24.1",
    "electron": "1.6.11",
    "electron-builder": "^19.12.0",
    "electron-packager": "8.7.1",
    "electron-prebuilt": "1.4.13",
    "electron-updater": "^2.4.4",
    "grunt": "1.0.1",
    "grunt-babel": "6.0.0",
    "grunt-cli": "1.2.0",
    "grunt-contrib-clean": "1.1.0",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-watch": "1.0.0",
    "grunt-newer": "1.3.0",
    "load-grunt-tasks": "3.5.2"
  },
  "build": {
    "appId": "es.albertoluna.elitetime",
    "directories": {
      "buildResources": "resources"
    }
  }
}
